python train_dtp.py -learning_rate 0.001 -embedding phoc -dataset botany -fold 1 -dtp_train 1 -augment 0 -verbose 1 -quiet 0 -eval_first_iteration 0 -weights model/ctrlfnet_mini_phoc_iiit_hws.pt -max_iters 35000 -eval_every 500 -h5 0 -image_size 1000 -save_id test_v4b -num_worker 0 > train/botany/botany_phoc_v4b.out 2> train/botany/botany_phoc_v4b.err